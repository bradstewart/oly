{
  "testEnvironment": "node",
  "testRegex": "(Test)\\.(ts|tsx)$",
  "transform": {
    ".(ts|tsx)": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
  },
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json",
    "ts",
    "tsx"
  ],
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/../oly-core/polyfill"
}
