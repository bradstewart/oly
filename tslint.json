{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "jsx-no-multiline-js": false,
    "object-literal-sort-keys": false,
    "ban-types": false,
    "max-classes-per-file": false,
    "member-access": false,
    "no-string-literal": false,
    "no-var-requires": false,
    "no-empty-interface": false,
    "variable-name": false
  }
}
