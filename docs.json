{
  "name": "oly",
  "modules": [
    {
      "name": "oly",
      "icon": "sim-card",
      "decorators": [
        "kernel/decorators/inject.ts",
        "kernel/decorators/state.ts",
        "kernel/decorators/env.ts",
        "kernel/decorators/on.ts"
      ],
      "services": [
        "kernel/Kernel.ts",
        "logger/Logger.ts"
      ],
      "exceptions": [
        "exception/Exception.ts"
      ]
    },
    {
      "name": "oly-json",
      "icon": "ninja",
      "decorators": [
        "decorators/field.ts",
        "decorators/array.ts",
        "decorators/date.ts",
        "decorators/schema.ts"
      ],
      "services": [
        "services/Json.ts"
      ]
    },
    {
      "name": "oly-http",
      "icon": "exchange",
      "services": [
        "providers/HttpServerProvider.ts",
        "services/HttpClient.ts"
      ],
      "exceptions": [
        "exceptions/HttpClientException.ts",
        "exceptions/HttpServerException.ts"
      ]
    },
    {
      "name": "oly-react",
      "icon": "application",
      "manuals": [
        "docs/Routing.md"
      ],
      "decorators": [
        "core/decorators/attach.ts",
        "core/decorators/action.ts",
        "router/decorators/page.ts",
        "router/decorators/layout.ts",
        "router/decorators/query.ts",
        "router/decorators/param.ts"
      ],
      "components": [
        "router/components/Go.tsx",
        "router/components/View.tsx",
        "core/components/AppContext.tsx"
      ],
      "services": [
        "router/services/Router.ts",
        "pixie/services/PixieStore.ts",
        "pixie/services/PixieHttp.ts",
        "pixie/services/PixieSession.ts",
        "router/providers/ReactBrowserProvider.ts",
        "server/providers/ReactServerProvider.ts",
        "router/providers/ReactRouterProvider.ts"
      ]
    },
    {
      "name": "oly-api",
      "icon": "satellite",
      "manuals": [
        "docs/Koa.md",
        "docs/Context.md"
      ],
      "decorators": [
        "core/decorators/get.ts",
        "core/decorators/post.ts",
        "core/decorators/put.ts",
        "core/decorators/del.ts",
        "core/decorators/body.ts",
        "core/decorators/param.ts",
        "core/decorators/query.ts",
        "core/decorators/router.ts",
        "core/decorators/use.ts"
      ],
      "services": [
        "core/providers/ApiProvider.ts",
        "core/services/ApiMiddlewares.ts",
        "swagger/providers/SwaggerProvider.ts"
      ]
    },
    {
      "name": "oly-security",
      "icon": "id-number",
      "decorators": [
        "decorators/auth.ts"
      ],
      "services": [
        "services/Crypto.ts",
        "services/JwtAuth.ts"
      ]
    },
    {
      "name": "oly-tools",
      "icon": "wrench",
      "manuals": [
        "docs/TypeScript.md",
        "docs/Webpack.md",
        "docs/Jest.md",
        "docs/Docker.md"
      ]
    }
  ]
}
