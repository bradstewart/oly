{
  "name": "oly",
  "manuals": [
    "manuals/concepts.md"
  ],
  "modules": [
    {
      "name": "oly-core",
      "decorators": [
        "decorators/env",
        "decorators/inject",
        "decorators/on",
        "decorators/state"
      ],
      "services": [
        "Kernel.ts",
        "logger/Logger.ts"
      ]
    },
    {
      "name": "oly-http",
      "decorators": [],
      "services": [
        "providers/HttpServerProvider.ts",
        "services/HttpClient.ts"
      ]
    },
    {
      "name": "oly-mapper",
      "decorators": [
        "decorators/array.ts",
        "decorators/field.ts"
      ],
      "services": [
        "ObjectMapper.ts"
      ]
    },
    {
      "name": "oly-api",
      "decorators": [
        "decorators/router",
        "decorators/get",
        "decorators/put",
        "decorators/post",
        "decorators/del",
        "decorators/path",
        "decorators/query",
        "decorators/body",
        "decorators/use"
      ],
      "services": [
        "providers/ApiProvider.ts"
      ]
    },
    {
      "name": "oly-react",
      "decorators": [
        "core/decorators/action",
        "core/decorators/attach",
        "router/decorators/page",
        "router/decorators/page404",
        "router/decorators/page500",
        "router/decorators/pageLayout",
        "router/decorators/query",
        "router/decorators/path"
      ],
      "services": [
        "router/providers/ReactBrowserProvider.ts",
        "router/services/Browser.ts",
        "router/services/Router.ts"
      ]
    }
  ]
}
