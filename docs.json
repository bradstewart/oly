{
  "name": "oly",
  "modules": [
    {
      "name": "oly-core",
      "icon": "sim-card",
      "manuals": [
        "docs/Glossary.md",
        "docs/Test.md"
      ],
      "decorators": [
        "kernel/decorators/inject.ts",
        "kernel/decorators/injectable.ts",
        "kernel/decorators/state.ts",
        "kernel/decorators/env.ts",
        "kernel/decorators/on.ts"
      ],
      "services": [
        "kernel/Kernel.ts",
        "logger/Logger.ts"
      ],
      "exceptions": [
        "exception/Exception.ts"
      ]
    },
    {
      "name": "oly-json",
      "icon": "ninja",
      "decorators": [
        "decorators/field.ts",
        "decorators/array.ts",
        "decorators/date.ts",
        "decorators/build.ts"
      ],
      "services": [
        "services/Json.ts"
      ]
    },
    {
      "name": "oly-http",
      "icon": "exchange",
      "services": [
        "providers/HttpServerProvider.ts",
        "services/HttpClient.ts"
      ],
      "exceptions": [
        "exceptions/HttpClientException.ts",
        "exceptions/HttpServerException.ts"
      ]
    },
    {
      "name": "oly-react",
      "icon": "application",
      "manuals": [
        "docs/Routing.md"
      ],
      "decorators": [
        "core/decorators/attach.ts",
        "router/decorators/page.ts",
        "router/decorators/layout.ts"
      ],
      "components": [
        "router/components/Go.tsx",
        "router/components/View.tsx",
        "core/components/AppContext.tsx"
      ],
      "services": [
        "router/services/Router.ts",
        "router/services/Browser.ts",
        "router/providers/ReactRouterProvider.ts",
        "router/providers/ReactBrowserProvider.ts"
      ]
    },
    {
      "name": "oly-react-ssr",
      "icon": "applications",
      "manuals": [
        "docs/Example.md"
      ],
      "services": [
        "server/providers/ReactServerProvider.ts",
        "pixie/providers/PixieBrowserProvider.ts",
        "pixie/providers/PixieServerProvider.ts",
        "pixie/services/PixieHttp.ts",
        "pixie/services/Pixie.ts",
        "pixie/services/PixieSession.ts"
      ]
    },
    {
      "name": "oly-api",
      "icon": "satellite",
      "decorators": [
        "decorators/get.ts",
        "decorators/post.ts",
        "decorators/put.ts",
        "decorators/del.ts",
        "decorators/body.ts",
        "decorators/param.ts",
        "decorators/header.ts",
        "decorators/query.ts",
        "decorators/router.ts",
        "decorators/file.ts",
        "decorators/use.ts"
      ],
      "services": [
        "providers/ApiProvider.ts",
        "services/ApiMiddlewares.ts"
      ]
    },
    {
      "name": "oly-security",
      "icon": "id-number",
      "exceptions": [
        "exceptions/JsonWebTokenException.ts",
        "exceptions/TokenExpiredException.ts"
      ],
      "decorators": [
        "decorators/auth.ts"
      ],
      "services": [
        "services/Crypto.ts",
        "services/JwtAuth.ts"
      ]
    },
    {
      "name": "oly-swagger",
      "icon": "manual",
      "decorators": [
        "decorators/api.ts"
      ],
      "services": [
        "providers/SwaggerProvider.ts"
      ]
    },
    {
      "name": "oly-amqp",
      "icon": "stacked-chart",
      "decorators": [
        "decorators/task.ts"
      ],
      "services": [
        "providers/AmqpProvider.ts",
        "providers/WorkerProvider.ts",
        "services/AmqpClient.ts"
      ]
    },
    {
      "name": "oly-tools",
      "icon": "wrench",
      "manuals": [
        "docs/Webpack.md"
      ]
    }
  ]
}
